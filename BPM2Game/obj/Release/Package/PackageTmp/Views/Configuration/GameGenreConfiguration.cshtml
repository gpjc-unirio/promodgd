@model IEnumerable<Bpm2GP.Model.DataBase.Models.GameGenre>

@{
    ViewBag.Title = "Game Genres";
}

<h3>Game Genres</h3>
<div class="col-md-12">
    <ol class="breadcrumb">
        <li class=""><a href="@Url.Action("Index", "Home")">Home</a></li>
        <li class="active">Game Genres</li>
    </ol>
</div>

<script>
    $(document).ready(function () {
        $('[data-toggle="tooltip"]').tooltip();
    });

    function OpenElementTab() {
        $("#tblGenres").removeClass("col-md-12");
        $("#tblGenres").addClass("col-md-6");
        $("#btnReloadLanguages").addClass("hide");
        $("#btnAddLanguage").addClass("hide");
        $(".btnShowElement").addClass("hide");
        $("#tblGenreElements").removeClass("hide");
        $("#tblGenreElements").attr("style", "border-left: 1px solid silver");
    }
</script>


<div id="tblGenres" class="col-md-12">
    @Html.Partial("_TblGenres", Model)
</div>
<div id="tblGenreElements" class="col-md-6 hide">

</div>
<div class="container">

</div>

