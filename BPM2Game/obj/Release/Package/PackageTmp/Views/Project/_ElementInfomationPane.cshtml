@model IEnumerable<Bpm2GP.Model.DataBase.Models.GameDesignMappingElements>

<div class="panel panel-primary">
    <div class="panel-heading">
        <h4>Element: <strong>@ViewBag.idModel</strong></h4>
    </div>
    <div class="panel-body">
        <table class="table table-striped table-bordered table-responsive">
            <tr>
                <th>
                    Process Element
                </th>
                <th>
                    Game Element
                </th>
                <th>
                    Mapped Element
                </th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @(item.AssociateElement.ProcessElement.Name)
                    </td>
                    <td>
                        @(item.GameGenreElement.Name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Descricao)
                    </td>
                </tr>
            }
        </table>    
    </div>
</div>